<mapper namespace="com.xmg.pss.mapper.DepartmentMapper">


	<insert id="save" useGeneratedKeys="true" 
	keyColumn="id" keyProperty="id">
	
		insert into department(name,sn) values(#{name},#{sn})
	
	
	</insert>	
	
</mapper>